{
  "name": "switcho",
  "version": "1.0.6",
  "description": "A simple utility that improves JavaScript's built-in \"switch\" statement.",
  "main": "dist/index.umd.min.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "declaration": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/il20afar/switcho.git"
  },
  "keywords": [
    "switch",
    "cases",
    "if",
    "else"
  ],
  "author": "il20afar <antoinefarley@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/il20afar/switcho/issues"
  },
  "homepage": "https://github.com/il20afar/switcho#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "rollup": "^2.51.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/ --ext .ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write . ",
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "typescript": "^4.3.2"
  }
}
